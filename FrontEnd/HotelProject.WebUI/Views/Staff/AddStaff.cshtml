@model AddStaffViewModel
@{
    ViewData["Title"] = "AddStaff";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<form method="post">
    <label>Personel Adı</label>
    <input type="text" asp-for="Name" class="form-control" />
    <br />

    <label>Personel Unvanı</label>
    <input type="text" asp-for="Title" class="form-control" />
    <br />
    
    <label>Facebook Adresi</label>
    <input type="text" asp-for="StaffFace" class="form-control" />
    <br />
    
    <label>Twitter Adresi</label>
    <input type="text" asp-for="StaffTwitter" class="form-control" />
    <br />
    
    <label>Instagram Adresi</label>
    <input type="text" asp-for="StaffInstagram" class="form-control" />
    <br />
    <button class="btn btn-primary">Personel Ekle</button>
</form>